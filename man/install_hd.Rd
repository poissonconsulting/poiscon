\name{install_hd}
\alias{install_hd}
\title{Install package from hard drive}
\usage{
install_hd(pkg = "poiscon", code_dir = getOption("code_dir",
  "~/Documents/code"))
}
\arguments{
  \item{pkg}{string of repository directory name}

  \item{code_dir}{string of path to repository directory}
}
\value{
Installs package
}
\description{
Install package from hard drive
}

